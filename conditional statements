import java.util.Scanner;

public class Sample {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a word:");
		String a= sc.nextLine();
		if (a.length() > 5) {
    System.out.println("String length is more than 5");
}		
		}

	}


