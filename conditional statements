		Scanner sc = new Scanner(System.in);
		System.out.println("enter electrical unit");
		int n= sc.nextInt();
		if(n>500) {
			System.out.println("unit greater 500");
		}
		else {
			System.out.println("unit less than 500");
		}
	}

