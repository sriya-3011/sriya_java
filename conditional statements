package java1;
import java.util.Scanner;

public class Demo5 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number:");
		int num=sc.nextInt();
		if(num>=10&&num<=99) {
			System.out.println("Two-digit number");
		}
		else {
			System.out.println("Not a two-digit number");
		}
		
		
		

	}

}
